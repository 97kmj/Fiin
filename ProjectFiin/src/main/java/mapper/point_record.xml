<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="mapper.pointRecord">
	<insert id="insertPointRecordAd">
		insert into point_record (advertiser_num, change_point)
		values(#{advertiser_num},#{point_amount})
	</insert>
	<insert id="insertPointRecordInf">
		insert into point_record (influencer_num, change_point)
		values(#{influencer_num},#{point_amount})
	</insert>

</mapper>